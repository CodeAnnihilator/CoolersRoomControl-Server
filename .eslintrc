{
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["eslint-plugin-import"],
    "rules": {
		"indent": ["warn", "tab", {"SwitchCase": 1}],
		"no-tabs": 0,
		"no-console": "warn"
    },
    "env": {
		"node": true,
		"es6": true
    }
}
